<?xml version="1.0" encoding="utf-8"?>  
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
	<item android:id="@android:id/background">
		<shape>
			<corners android:radius="8.0dip" />
			<gradient android:startColor="@color/gridselect" android:endColor="@color/gridselect"
			android:angle="270.0" />
		</shape>
	</item>
	<item android:id="@android:id/progress">
		<clip>
			<shape>
			<corners android:radius="0.0dip" />
			<gradient android:startColor="@color/textGreen" android:endColor="@color/textGreen"
			android:centerColor="@color/textGreen" android:angle="270.0" />
			</shape>
		</clip>
	</item>

</layer-list>
